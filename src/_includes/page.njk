{% extends "layout.njk" %}

{% block inner %}

    {{ c('header', {
        title: title,
        event: event,
        intro: intro,
        isHome: false,
        navigation: navigation.header,
        currentUrl: page.url
    }) }}

    <main class="o-wrap o-wrap--{{ contentSize }}">
        {{ c('content', { content: content, beforeContent: beforeContent, afterContent: afterContent, style: contentStyle }) }}
    </main>

    {{ c('footer', { navigation: navigation.footer, event: event,isHome: false }) }}

{% endblock %}